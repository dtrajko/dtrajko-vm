session.hash_function = {{ php_ini_options.session_hash_function | default('whirlpool') }}

session.name = {{ php_ini_options.session_name | default('PHPSESSID') }}
session.referer_check =
session.save_handler = {{ php_ini_options.session_save_handler | default('files') }}
session.save_path = {{ php_ini_options.session_save_path | default('/tmp') }}
